<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="applications.linegraph">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".FirstActivity"
            android:parentActivityName=".SupahActivity" />
        <activity
            android:name=".GraphActivity"
            android:parentActivityName=".FirstActivity" />
        <activity android:name=".SupahActivity"
            android:label="app">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SecondActivity"
            android:parentActivityName=".SupahActivity"/>
        <activity android:name=".DichoActivity"
            android:parentActivityName=".SecondActivity"/>
        <activity android:name=".ThirdActivity"
            android:parentActivityName=".SupahActivity"/>
        <activity android:name=".DerivActivity"
            android:parentActivityName=".ThirdActivity"></activity>
    </application>

</manifest>